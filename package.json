    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|webp|svg|css|scss)$": "identity-obj-proxy"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/",
      "<rootDir>/static/"
    ]
  }
