    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.5",
    "@reduxjs/toolkit": "1.9.5",
    "antd": "^5.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.1.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
