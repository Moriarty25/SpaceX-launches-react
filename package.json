  },
  "homepage": "https://github.com/Moriarty25/SpaceX-launches-react#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.3",
    "@types/react-dom": "^18.2.7",
    "@types/react-redux": "7.1.25",
    "cross-fetch": "^4.0.0",
    "css-loader": "^6.8.1",
