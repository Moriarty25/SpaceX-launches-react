{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "allowJs": true,
    "sourceMap": true,
		"moduleResolution": "Node",
    "baseUrl": "./",
    "outDir": "dist/",
    "esModuleInterop": true,
    "noEmit": false,
    "resolveJsonModule": true,
		"jsx": "react-jsx"
  },
	"include": [
		"src"
	],
  "exclude": [
		"node_modules",
		"**/test.ts",
		"**/test.tsx"
	]
}
